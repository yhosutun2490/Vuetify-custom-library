{
  "name": "@yhosutun2490/custom-vuetify-ui-library",
  "version": "0.3.0",
  "description": "custom-vuetify-ui-library",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/main/index.cjs.js",
  "module": "./dist/main/index.js",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/main/index.js",
      "require": "./dist/main/index.cjs.js",
      "style": "./dist/main/style.css"
    },
    "./Paginator": {
      "import": "./dist/Paginator/index.js",
      "require": "./dist/Paginator/index.cjs.js"
    },
    "./Paginator/style": {
      "import": "./dist/Paginator/style.css"
    },
    "./Button": {
      "import": "./dist/Button/index.js",
      "require": "./dist/Button/index.cjs.js",
      "style": "./dist/Button/style.css"
    },
    "./Button/style": {
      "import": "./dist/Button/style.css"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "vite --port 3006",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "core-js": "^3.37.1",
    "roboto-fontface": "*"
  },
  "peerDependencies": {
    "vue": "^3.5.12",
    "vuetify": "^3.6.11"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-onboarding": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@storybook/vue3": "^8.4.2",
    "@storybook/vue3-vite": "^8.4.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-storybook": "^0.11.0",
    "eslint-plugin-vue": "^9.27.0",
    "sass": "1.77.6",
    "storybook": "^8.4.2",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.3.3",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-router": "^4.4.0"
  }
}
